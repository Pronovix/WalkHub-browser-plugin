{
	"manifest_version": 2,

	"name": "WalkHub",
	"description": "Allows you to play Walkthroughs on any page without a proxy.",
	"version": "1.0",
	"homepage_url": "http://walkhub.net",
	"browser_action": {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "logo.png",
		"48": "logo.png",
		"128": "logo.png"
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["walkhub_loader.js"],
			"all_frames": true
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"tabs",
		"*://*/*",
		"storage",
		"<all_urls>"
	]
}
